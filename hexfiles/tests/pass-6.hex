; This is basic file uses the memory mapped console input and output.
#NOFINALSTATE

START:
a9 'H'    ; LDA 'H'
85 FA     ; STA $FA ; Write to stdout
a9 'e'    ; LDA 'e'
85 FA     ; STA $FA ; Write to stdout
a9 'l'    ; LDA 'l'
85 FA     ; STA $FA ; Write to stdout
85 FA     ; STA $FA ; Write to stdout
a9 'o'    ; LDA 'o'
85 FA     ; STA $FA ; Write to stdout
a9 ' '    ; LDA ' '
85 FA     ; STA $FA ; Write to stdout
a9 'W'    ; LDA 'W'
85 FA     ; STA $FA ; Write to stdout
a9 'o'    ; LDA 'o'
85 FA     ; STA $FA ; Write to stdout
a9 'r'    ; LDA 'r'
85 FA     ; STA $FA ; Write to stdout
a9 'l'    ; LDA 'l'
85 FA     ; STA $FA ; Write to stdout
a9 'd'    ; LDA 'd'
85 FA     ; STA $FA ; Write to stdout
a9 '!'    ; LDA '!'
85 FA     ; STA $FA ; Write to stdout

; Write a new line.
a9 10     ; LDA LF
85 FA     ; STA $FA ; Write to stdout

STOP:
